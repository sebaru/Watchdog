	jnz _fct_sortie_tor_pas_6
	ljmp _fct_sortie_tor_6_ana_tor
_fct_sortie_tor_pas_6:

	subb A,#0x01
	jnz _fct_sortie_tor_pas_7
	ljmp _fct_sortie_tor_7_ana_tor
_fct_sortie_tor_pas_7:

